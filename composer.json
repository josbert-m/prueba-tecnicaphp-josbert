{
    "require": {
        "php": "^7.4",
        "vlucas/phpdotenv": "^5.5",
        "nesbot/carbon": "^2.64",
        "fakerphp/faker": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
